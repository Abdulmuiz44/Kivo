{
  "name": "kivo-mobile",
  "slug": "kivo-mobile",
  "version": "2.0.0",
  "orientation": "portrait",
  "icon": "./assets/icon.png",
  "userInterfaceStyle": "automatic",
  "splash": {
    "image": "./assets/splash.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  },
  "assetBundlePatterns": ["**/*"],
  "ios": {
    "supportsTablet": true,
    "bundleIdentifier": "com.kivo.mobile"
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "package": "com.kivo.mobile"
  },
  "web": {
    "favicon": "./assets/favicon.png"
  },
  "scheme": "kivo",
  "plugins": ["expo-router"],
  "experiments": {
    "typedRoutes": true
  }
}
